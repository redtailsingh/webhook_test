//5
pipeline {
    agent any
    stages {
        stage('say hello') {
            steps {
                echo 'hello there!'
		githubNotify account: 'redtailsingh', context: 'jenkins/build', credentialsId: 'redtailsingh', description: 'this is an example', repo: "${GIT_BRANCH}", sha: "${GIT_COMMIT}", status: 'SUCCESS', targetUrl:'http://192.168.82.1:8080/job/Redtail-Retriever/'
            }
        }
        stage('say bye') {
            steps {
                echo 'bye there!! '
            }
        }
    }
}
